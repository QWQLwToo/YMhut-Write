{
  "name": "ymhut-write",
  "version": "1.0.0",
  "main": "electron/main.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently -k \"npm run vite\" \"npm run electron\"",
    "vite": "vite",
    "electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps && electron-rebuild -f -w better-sqlite3",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "dayjs": "^1.11.0",
    "docx": "^9.5.1",
    "easymde": "^2.20.0",
    "fs-extra": "^11.0.0",
    "gray-matter": "^4.0.3",
    "ini": "^4.0.0",
    "mammoth": "^1.11.0",
    "markdown-it": "^13.0.0",
    "pinia": "^2.1.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.0"
  }
}
